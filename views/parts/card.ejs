
<div style="background-color:whitesmoke !important">
  
  
    <% data.map(bot => {%>
    
    
    <% 
    let servers;
    if(bot.server_count == 0) {
      servers = "N/A"
    } else {
      servers = bot.server_count  + "  servers"
    };
    let status;
    if(_client.users.cache.has(bot.id)) {
       status = _client.users.cache.get(bot.id).presence.status;
    } else {
      status = "offline"
    };
    %>
  
    
  <span class="rw">
