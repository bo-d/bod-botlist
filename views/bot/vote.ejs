    <div style="color:black; text-align: center" id="txt"></div>
    <center><div class="btn btn-success" id="bt" onclick="gg()">VOTE</div></center>
    <br>
    <center>
    <button onclick="window.location.href = '/bots/<%= data.id %>'" class="btn btn-primary">Back to <%=data.name%></button>
    </center>
    
  </div>
  
<script>
function gg() {
  
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
 let txt = document.getElementById("txt");
      let bt = document.getElementById("bt");
    
      if(this.responseText === "100") {
        bt. style. display = "none";
  
        txt. innerHTML = "‚ô¶Ô∏è You can vote only once 24 hours. "
      };
      if(this. responseText === "50") {
        bt. style. display = "none";
        document. getElementById("txt").innerHTML = "üëç Successfully upvoted";
      };
    }
  };
  xhttp.open("POST", "vote", true);
  xhttp.send();
}
  </script>
  
  

